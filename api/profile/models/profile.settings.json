{
  "connection": "default",
  "collectionName": "profiles",
  "info": {
    "name": "profile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "profile"
    },
    "likes": {
      "collection": "article"
    },
    "downloads": {
      "collection": "article"
    },
    "shares": {
      "collection": "article"
    },
    "picture": {
      "type": "richtext"
    }
  }
}
